<?xml version="1.0"?>
<!DOCTYPE codetemplates PUBLIC "-//NetBeans//DTD Editor Code Templates settings 1.0//EN" "http://www.netbeans.org/dtds/EditorCodeTemplates-1_0.dtd">
<codetemplates>
    <codetemplate contexts="ownlang-code" abbreviation="def">
        <code><![CDATA[def ${name}(${args}) {
  ${cursor}
}]]></code>
        <description>Defines new function</description>
    </codetemplate>
    <codetemplate contexts="ownlang-code" abbreviation="match">
        <code><![CDATA[match ${name} {
  case ${e1}: ${e1}
  case ${e2}: ${e2}
  case _: ${result}
}]]></code>
        <description>match</description>
    </codetemplate>
    <codetemplate contexts="ownlang-code" abbreviation="map">
        <code><![CDATA[{
  "${key1}": ${value1},
  "${key2}": ${value2}
}]]></code>
        <description>map</description>
    </codetemplate>
    <codetemplate contexts="ownlang-code" abbreviation="echo">
        <code><![CDATA[echo ("${a} = ", ${a})]]></code>
        <description>echo variable</description>
    </codetemplate>
    <codetemplate contexts="ownlang-code" abbreviation="for">
        <code><![CDATA[for (${a} = ${0}, ${a} < ${10}, ${a}++) {
  ${cursor}
}]]></code>
        <description>for loop</description>
    </codetemplate>
</codetemplates>